{
  "name": "com_res",
  "version": "0.0.0",
  "description": "com_res",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "test:unit": "cypress run --component",
    "test:unit:dev": "cypress open --component",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --ignore-path .gitignore",
    "lint-fix": "eslint --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore .",
    "format": "prettier --check src/",
    "format-fix": "prettier --write src/",
    "lint-format": "npm-run-all --parallel lint format",
    "lint-format-fix": "npm-run-all --parallel lint-fix format-fix",
    "precommit": "npm run lint-format",
    "prepush": "npm run lint-format",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.2",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@vueuse/core": "^10.5.0",
    "chart.js": "^4.4.9",
    "chartjs-adapter-date-fns": "^3.0.0",
    "esri-leaflet": "^3.0.12",
    "esri-leaflet-geocoder": "^3.1.5",
    "georaster": "^1.6.0",
    "georaster-layer-for-leaflet": "^3.10.0",
    "leaflet": "^1.9.4",
    "leaflet-easybutton": "^2.4.0",
    "loadash": "^1.0.0",
    "lorem-ipsum": "^2.0.8",
    "pinia": "^3.0.3",
    "proj4": "^2.19.5",
    "proj4-fully-loaded": "^0.2.0",
    "swagger-ui": "^5.9.0",
    "vite-plugin-vuetify": "^2.1.1",
    "vue": "^3.3.4",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.2.4",
    "vuetify": "^3.8.12"
  },
  "devDependencies": {
    "@mdi/font": "^7.3.67",
    "@mdi/js": "^7.3.67",
    "@rushstack/eslint-patch": "^1.3.3",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "cypress": "^14.5.1",
    "eslint": "^8.57.1",
    "eslint-plugin-cypress": "^2.15.2",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "npm-run-all": "^4.1.5",
    "pm2": "^6.0.5",
    "prettier": "^3.0.3",
    "sass": "^1.69.3",
    "start-server-and-test": "^2.0.0",
    "vite": "^7.0.0",
    "vite-plugin-vue-devtools": "^8.0.0"
  },
  "overrides": {
    "georaster-layer-for-leaflet": {
      "proj4-fully-loaded": "^0.2.0",
      "reproject-bbox": {
        "proj4-fully-loaded": "^0.2.0"
      },
      "geocanvas": {
        "geomask": {
          "reproject-geojson": {
            "proj4-fully-loaded": "^0.2.0"
          }
        }
      }
    },
    "reproject-geojson": {
      "proj4-fully-loaded": "^0.2.0"
    },
    "reproject-bbox": {
      "proj4-fully-loaded": "^0.2.0"
    }
